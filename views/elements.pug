                  span.badge 42
                br
                br
                button.btn.btn-primary(type='button')
                  | Messages 
                  span.badge 4
            // END LABELS
            // PROGRESS BARS
            .panel
              .panel-heading
                h3.panel-title Progress Bars
              .panel-body
                .progress
                  .progress-bar(role='progressbar' aria-valuenow='60' aria-valuemin='0' aria-valuemax='100' style='width: 60%;')
                    span.sr-only 60% Complete
                .progress
                  .progress-bar.progress-bar-warning(role='progressbar' aria-valuenow='60' aria-valuemin='0' aria-valuemax='100' style='width: 60%')
                    span.sr-only 60% Complete (warning)
                .progress
                  .progress-bar.progress-bar-success(role='progressbar' aria-valuenow='60' aria-valuemin='0' aria-valuemax='100' style='width: 60%')
                    span.sr-only 60% Complete (success)
                .progress
                  .progress-bar.progress-bar-danger(role='progressbar' aria-valuenow='60' aria-valuemin='0' aria-valuemax='100' style='width: 60%')
                    span.sr-only 60% Complete (danger)
                .progress
                  .progress-bar(role='progressbar' aria-valuenow='75' aria-valuemin='0' aria-valuemax='100' style='width: 75%;')
                    | 75%
                .progress
                  .progress-bar.progress-bar-info.progress-bar-striped(role='progressbar' aria-valuenow='20' aria-valuemin='0' aria-valuemax='100' style='width: 20%')
                    span.sr-only 20% Complete
                .progress
                  .progress-bar.progress-bar-striped.active(role='progressbar' aria-valuenow='45' aria-valuemin='0' aria-valuemax='100' style='width: 45%')
                    span.sr-only 45% Complete
                .progress.progress-xs
                  .progress-bar.progress-bar-success(role='progressbar' aria-valuenow='80' aria-valuemin='0' aria-valuemax='100' style='width: 80%')
                    span.sr-only 80% Complete
            // END PROGRESS BARS
            // INPUT GROUPS
            .panel
              .panel-heading
                h3.panel-title Input Groups
              .panel-body
                .input-group
                  input.form-control(type='text')
                  span.input-group-btn
                    button.btn.btn-primary(type='button') Go!
                br
                .input-group
                  span.input-group-btn
                    button.btn.btn-primary(type='button') Go!
                  input.form-control(type='text')
                br
                .input-group
                  span.input-group-addon
                    i.fa.fa-user
                  input.form-control(placeholder='Username' type='text')
                br
                .input-group
                  input.form-control(placeholder='Username' type='text')
                  span.input-group-addon
                    i.fa.fa-user
                br
                .input-group
                  span.input-group-addon $
                  input.form-control(type='text')
                  span.input-group-addon .00
            // END INPUT GROUPS
            // ALERT MESSAGES
            .panel
              .panel-heading
                h3.panel-title Alert Messages
              .panel-body
                .alert.alert-info.alert-dismissible(role='alert')
                  button.close(type='button' data-dismiss='alert' aria-label='Close')
                    span(aria-hidden='true') &times;
                  i.fa.fa-info-circle
                  |  The system is running well
                .alert.alert-success.alert-dismissible(role='alert')
                  button.close(type='button' data-dismiss='alert' aria-label='Close')
                    span(aria-hidden='true') &times;
                  i.fa.fa-check-circle
                  |  Your settings have been succesfully saved
                .alert.alert-warning.alert-dismissible(role='alert')
                  button.close(type='button' data-dismiss='alert' aria-label='Close')
                    span(aria-hidden='true') &times;
                  i.fa.fa-warning
                  |  Warning, check your permission settings
                .alert.alert-danger.alert-dismissible(role='alert')
                  button.close(type='button' data-dismiss='alert' aria-label='Close')
                    span(aria-hidden='true') &times;
                  i.fa.fa-times-circle
                  |  Your account has been suspended
            // END ALERT MESSAGES
