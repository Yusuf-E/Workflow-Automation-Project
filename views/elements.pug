  // MAIN
  .main
    // MAIN CONTENT
    .main-content
      .container-fluid
        h3.page-title Elements
        .row
          .col-md-6
            // BUTTONS
            .panel
              .panel-heading
                h3.panel-title Buttons
              .panel-body
                p.demo-button
                  button.btn.btn-default(type='button') Default
                  button.btn.btn-primary(type='button') Primary
                  button.btn.btn-info(type='button') Info
                  button.btn.btn-success(type='button') Success
                  button.btn.btn-warning(type='button') Warning
                  button.btn.btn-danger(type='button') Danger
                br
                p.demo-button
                  button.btn.btn-primary.btn-lg(type='button') Large Size
                  button.btn.btn-primary(type='button') Default Size
                  button.btn.btn-primary.btn-sm(type='button') Small Size
                  button.btn.btn-primary.btn-xs(type='button') Extra Small Size
                br
                p.demo-button
                  button.btn.btn-default(type='button')
                    i.fa.fa-plus-square
                    |  Default
                  button.btn.btn-primary(type='button')
                    i.fa.fa-refresh
                    |  Primary
                  button.btn.btn-info(type='button')
                    i.fa.fa-info-circle
                    |  Info
                  button.btn.btn-primary(type='button' disabled='disabled')
                    i.fa.fa-refresh.fa-spin
                    |  Refreshing...
                br
                p.demo-button
                  button.btn.btn-success(type='button')
                    i.fa.fa-check-circle
                    |  Success
                  button.btn.btn-warning(type='button')
                    i.fa.fa-warning
                    |  Warning
                  button.btn.btn-danger(type='button')
                    i.fa.fa-trash-o
                    |  Danger
                  button.btn.btn-primary(type='button' disabled='disabled')
                    i.fa.fa-spinner.fa-spin
                    |  Loading...
                br
                .row
                  .col-md-6
                    button.btn.btn-primary.btn-block(type='button') Button Block
                  .col-md-6
                    button.btn.btn-warning.btn-block(type='button') Button Block
            // END BUTTONS
            // INPUTS
            .panel
              .panel-heading
                h3.panel-title Inputs
              .panel-body
                input.form-control(type='text' placeholder='text field')
                br
                input.form-control(type='password' value='asecret')
                br
                textarea.form-control(placeholder='textarea' rows='4')
                br
                select.form-control
                  option(value='cheese') Cheese
                  option(value='tomatoes') Tomatoes
                  option(value='mozarella') Mozzarella
                  option(value='mushrooms') Mushrooms
                  option(value='pepperoni') Pepperoni
                  option(value='onions') Onions
