                label.fancy-checkbox
                  input(type='checkbox')
                  span Fancy Checkbox 1
                label.fancy-checkbox
                  input(type='checkbox')
                  span Fancy Checkbox 2
                label.fancy-checkbox
                  input(type='checkbox')
                  span Fancy Checkbox 3
                br
                label.fancy-radio
                  input(name='gender' value='male' type='radio')
                  span
                    i
                    | Male
                label.fancy-radio
                  input(name='gender' value='female' type='radio')
                  span
                    i
                    | Female
            // END INPUTS
            // INPUT SIZING
            .panel
              .panel-heading
                h3.panel-title Input Sizing
              .panel-body
                input.form-control.input-lg(placeholder='.input-lg' type='text')
                br
                input.form-control(placeholder='Default input' type='text')
                br
                input.form-control.input-sm(placeholder='.input-sm' type='text')
                br
                select.form-control.input-lg
                  option(value='cheese') Cheese
                  option(value='tomatoes') Tomatoes
                  option(value='mozarella') Mozzarella
                  option(value='mushrooms') Mushrooms
                  option(value='pepperoni') Pepperoni
                  option(value='onions') Onions
                br
                select.form-control
                  option(value='cheese') Cheese
                  option(value='tomatoes') Tomatoes
                  option(value='mozarella') Mozzarella
                  option(value='mushrooms') Mushrooms
                  option(value='pepperoni') Pepperoni
                  option(value='onions') Onions
                br
                select.form-control.input-sm
                  option(value='cheese') Cheese
                  option(value='tomatoes') Tomatoes
                  option(value='mozarella') Mozzarella
                  option(value='mushrooms') Mushrooms
                  option(value='pepperoni') Pepperoni
                  option(value='onions') Onions
            // END INPUT SIZING
          .col-md-6
            // LABELS
            .panel
              .panel-heading
                h3.panel-title Labels &amp; Badges
              .panel-body
                span.label.label-default DEFAULT
                span.label.label-primary PRIMARY
                span.label.label-success SUCCESS
                span.label.label-info INFO
                span.label.label-warning WARNING
                span.label.label-danger DANGER
