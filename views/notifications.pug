doctype html
head
  title Notifications | Klorofil - Free Bootstrap Dashboard Template
  meta(charset='utf-8')
  meta(http-equiv='X-UA-Compatible' content='IE=edge,chrome=1')
  meta(name='viewport' content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0')
  // VENDOR CSS
  link(rel='stylesheet' href='assets/vendor/bootstrap/css/bootstrap.min.css')
  link(rel='stylesheet' href='assets/vendor/font-awesome/css/font-awesome.min.css')
  link(rel='stylesheet' href='assets/vendor/linearicons/style.css')
  link(rel='stylesheet' href='assets/vendor/toastr/toastr.min.css')
  // MAIN CSS
  link(rel='stylesheet' href='assets/css/main.css')
  // FOR DEMO PURPOSES ONLY. You should remove this in your project
  link(rel='stylesheet' href='assets/css/demo.css')
  // GOOGLE FONTS
  link(href='https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700' rel='stylesheet')
  // ICONS
  link(rel='apple-touch-icon' sizes='76x76' href='assets/img/apple-icon.png')
  link(rel='icon' type='image/png' sizes='96x96' href='assets/img/favicon.png')
// WRAPPER
#wrapper
  // NAVBAR
  nav.navbar.navbar-default.navbar-fixed-top
    .brand
      a(href='index.html')
        img.img-responsive.logo(src='assets/img/logo-dark.png' alt='Klorofil Logo')
    .container-fluid
      .navbar-btn
        button.btn-toggle-fullwidth(type='button')
          i.lnr.lnr-arrow-left-circle
      form.navbar-form.navbar-left
        .input-group
          input.form-control(type='text' value='' placeholder='Search dashboard...')
          span.input-group-btn
            button.btn.btn-primary(type='button') Go
      .navbar-btn.navbar-btn-right
        a.btn.btn-success.update-pro(href='https://www.themeineed.com/downloads/klorofil-pro-bootstrap-admin-dashboard-template/?utm_source=klorofil&utm_medium=template&utm_campaign=KlorofilPro' title='Upgrade to Pro' target='_blank')
          i.fa.fa-rocket
          span UPGRADE TO PRO
      #navbar-menu
        ul.nav.navbar-nav.navbar-right
          li.dropdown
            a.dropdown-toggle.icon-menu(href='#' data-toggle='dropdown')
              i.lnr.lnr-alarm
              span.badge.bg-danger 5
            ul.dropdown-menu.notifications
              li
                a.notification-item(href='#')
                  span.dot.bg-warning
                  | System space is almost full
              li
                a.notification-item(href='#')
                  span.dot.bg-danger
                  | You have 9 unfinished tasks
              li
                a.notification-item(href='#')
                  span.dot.bg-success
                  | Monthly report is available
              li
                a.notification-item(href='#')
                  span.dot.bg-warning
                  | Weekly meeting in 1 hour
              li
                a.notification-item(href='#')
                  span.dot.bg-success
                  | Your request has been approved
              li
                a.more(href='#') See all notifications
          li.dropdown
            a.dropdown-toggle(href='#' data-toggle='dropdown')
              i.lnr.lnr-question-circle
              span Help
              i.icon-submenu.lnr.lnr-chevron-down
            ul.dropdown-menu
              li
                a(href='#') Basic Use
              li
                a(href='#') Working With Data
              li
                a(href='#') Security
              li
                a(href='#') Troubleshooting
          li.dropdown
            a.dropdown-toggle(href='#' data-toggle='dropdown')
              img.img-circle(src='assets/img/user.png' alt='Avatar')
              span Samuel
              i.icon-submenu.lnr.lnr-chevron-down
            ul.dropdown-menu
              li
                a(href='#')
                  i.lnr.lnr-user
                  span My Profile
              li
                a(href='#')
                  i.lnr.lnr-envelope
                  span Message
              li
                a(href='#')
                  i.lnr.lnr-cog
                  span Settings
              li
                a(href='#')
                  i.lnr.lnr-exit
                  span Logout
          //
            <li>
            <a class="update-pro" href="https://www.themeineed.com/downloads/klorofil-pro-bootstrap-admin-dashboard-template/?utm_source=klorofil&utm_medium=template&utm_campaign=KlorofilPro" title="Upgrade to Pro" target="_blank"><i class="fa fa-rocket"></i> <span>UPGRADE TO PRO</span></a>
            </li>
  // END NAVBAR
  // LEFT SIDEBAR
  #sidebar-nav.sidebar
    .sidebar-scroll
      nav
        ul.nav
          li
            a(href='index.html')
              i.lnr.lnr-home
              span Dashboard
          li
            a(href='elements.html')
              i.lnr.lnr-code
              span Elements
          li
            a(href='charts.html')
              i.lnr.lnr-chart-bars
              span Charts
          li
            a(href='panels.html')
              i.lnr.lnr-cog
              span Panels
          li
            a.active(href='notifications.html')
              i.lnr.lnr-alarm
              span Notifications
          li
            a.collapsed(href='#subPages' data-toggle='collapse')
              i.lnr.lnr-file-empty
              span Pages
              i.icon-submenu.lnr.lnr-chevron-left
            #subPages.collapse
              ul.nav
                li
                  a(href='page-profile.html') Profile
                li
                  a(href='page-login.html') Login
                li
                  a(href='page-lockscreen.html') Lockscreen
          li
            a(href='tables.html')
              i.lnr.lnr-dice
              span Tables
          li
            a(href='typography.html')
              i.lnr.lnr-text-format
              span Typography
          li
            a(href='icons.html')
              i.lnr.lnr-linearicons
              span Icons
  // END LEFT SIDEBAR
  // MAIN
  .main
    // MAIN CONTENT
    .main-content
      .container-fluid
        h3.page-title Notifications
        #toastr-demo.panel
          .panel-body
            // CONTEXTUAL
            h4 Context
            p.demo-button
              button.btn.btn-primary.btn-toastr(type='button' data-context='info' data-message='This is general theme info' data-position='top-right') General Info
              button.btn.btn-success.btn-toastr(type='button' data-context='success' data-message='This is success info' data-position='top-right') Success Info
              button.btn.btn-warning.btn-toastr(type='button' data-context='warning' data-message='This is warning info' data-position='top-right') Warning Info
              button.btn.btn-danger.btn-toastr(type='button' data-context='error' data-message='This is error info' data-position='top-right') Error Info
            // END CONTEXTUAL
            br
            // POSITION
            h4 Position
            p.demo-button
              button.btn.btn-default.btn-toastr(type='button' data-context='info' data-message="Hi, I'm here" data-position='bottom-right') Bottom Right
              button.btn.btn-default.btn-toastr(type='button' data-context='info' data-message="Hi, I'm here" data-position='bottom-left') Bottom Left
              button.btn.btn-default.btn-toastr(type='button' data-context='info' data-message="Hi, I'm here" data-position='top-left') Top Left
              button.btn.btn-default.btn-toastr(type='button' data-context='info' data-message="Hi, I'm here" data-position='top-right') Top Right
              button.btn.btn-default.btn-toastr(type='button' data-context='info' data-message="Hi, I'm here" data-position='top-full-width') Top Full Width
              button.btn.btn-default.btn-toastr(type='button' data-context='info' data-message="Hi, I'm here" data-position='bottom-full-width') Bottom Full Width
              button.btn.btn-default.btn-toastr(type='button' data-context='info' data-message="Hi, I'm here" data-position='top-center') Top Center
              button.btn.btn-default.btn-toastr(type='button' data-context='info' data-message="Hi, I'm here" data-position='bottom-center') Bottom Center
            // END POSITION
            br
            // CALLBACK
            h4 Callback
            p.demo-button
              button#toastr-callback1.btn.btn-default.btn-toastr-callback(type='button' data-context='info' data-message='onShown and onHidden callback demo') onShown and onHidden
              button#toastr-callback2.btn.btn-default.btn-toastr-callback(type='button' data-context='info' data-message='Please click me') onclick
              button#toastr-callback3.btn.btn-default.btn-toastr-callback(type='button' data-context='info' data-message='Please click close button') onCloseClick
            // END CALLBACK
    // END MAIN CONTENT
  // END MAIN
  .clearfix
  footer
    .container-fluid
      p.copyright
        | &copy; 2017 
        a(href='https://www.themeineed.com' target='_blank') Theme I Need
        | . All Rights Reserved.
// END WRAPPER
// Javascript
script(src='assets/vendor/jquery/jquery.min.js')
script(src='assets/vendor/bootstrap/js/bootstrap.min.js')
script(src='assets/vendor/jquery-slimscroll/jquery.slimscroll.min.js')
script(src='assets/vendor/toastr/toastr.min.js')
script(src='assets/scripts/klorofil-common.js')
